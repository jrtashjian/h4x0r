	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"bottom": {
						"style": "none",
						"width": "0px"
					},
					"left": {
						"style": "none",
						"width": "0px"
					},
					"radius": "0px",
					"right": {
						"style": "none",
						"width": "0px"
					},
					"top": {
						"style": "none",
						"width": "0px"
					}
				},
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--base)"
						}
					}
				},
				"shadow": "6px 6px 0px rgba(0, 0, 0, 0.2)",
				"spacing": {
					"padding": {
						"bottom": "1rem",
						"left": "1rem",
						"right": "1rem",
						"top": "1rem"
					}
				},
				"typography": {
					"fontSize": "1rem",
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "1",
					"textDecoration": "none",
					"textTransform": "none"
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "1rem"
				},
				"typography": {
					"textDecoration": "none"
				}
			},
			"core/code": {
				"border": {
					"left": {
						"color": "var(--wp--preset--color--base-muted)",
						"style": "solid",
						"width": "1ch"
					}
				},
				"spacing": {
					"padding": {
						"bottom": "0ch",
						"left": "1ch",
						"right": "1ch",
						"top": "0ch"
					}
				}
			},
			"core/heading": {
				"spacing": {
					"margin": {
						"bottom": "1rem",
						"top": "2rem"
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"button": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"link": {
				":active": {
					"typography": {
						"textDecoration": "none"
					}
				},
				":any-link": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "none"
					}
				},
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"spacing": {
			"blockGap": "calc(var(--wp--preset--font-size--normal) * 2)",
			"padding": {
				"bottom": "0px",
				"left": "var(--wp--preset--font-size--normal)",
				"right": "var(--wp--preset--font-size--normal)",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-monospace)",
			"fontSize": "var(--wp--preset--font-size--normal)",
			"lineHeight": "var(--wp--style--block-gap)"
		}
	},
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json"
}